rule Results2Table {
	
	template : "results2table.pinset"
	
	parameters : Map {
		"path" = Sequence{"Stats"},
		"icon" = "table",
		"format" = "csv"
	}
}

rule Results2Chart {

	parameters : Map {
		"results" = M!End_To_End_Flow_Result.all
	}
	
	template : "results2chart.egl"
	
	target : "charts/resultBarChart.html"
	
	
}
