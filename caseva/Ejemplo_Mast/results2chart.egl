<!doctype html>
<html lang=en>
<head>
	<meta charset="utf-8"/>
    <style>
        #barChart {
            margin: 0 auto;
        }
	</style>
</head>
<body>
	<h1>Barchart</h1>
	<div style="width: 800px;"><canvas id="barChart"></canvas></div>
	<script>
		[%var count= results.size;%]
		const labels = [
			[%var i = 0;%]
			[%for( result in results){%]
				'[%=result.Model_Elem.href%]'
				[%if(i>=count)%][%=','%]
				[%i++;%]
			[%}%]
		]
		const data = {
			labels: labels,
			datasets: [
				{
					label: 'b',
					data: [
					[%for( result in results){%]
						[%=result.Timing_Result.Worst_Blocking_Time.get(0)%],
					[%}%]
					backgroundColor: Utils.CHART_COLORS.red,
					]
				},
				{
					label: 'w',
					data: [
					[%for( result in results){%]
						[%=result.Slack%],
					[%}%]
					backgroundColor: Utils.CHART_COLORS.blue,
					]
				},
				{
					label: 'c',
					data: [
					[%for( result in results){%]
						[%=result.Timing_Result.Worst_Output_Jitter.get(0).Value.get(0)%],
					[%}%]
					backgroundColor: Utils.CHART_COLORS.green,
					]
				}
			]
		};
		const config = {
  			type: 'bar',
  			data: data,
  			options: {
    			plugins: {
      				title: {
        				display: true,
        				text: 'Slack'
      				},
    			},
    			responsive: true,
    			scales: {
      				x: {
        				stacked: true,
      				},
      				y: {
        				stacked: true
      				}
    			}
  			}
		};
	</script>
</body>
</html>