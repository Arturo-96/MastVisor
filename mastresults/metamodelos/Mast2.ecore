    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Avg_Context_Switch" ordered="false"
        lowerBound="1" eType="#//Normalized_Execution_Time" defaultValueLiteral="0.0">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Average overhead for a context switch between schedulable resources."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Best_Context_Switch" ordered="false"
        lowerBound="1" eType="#//Normalized_Execution_Time" defaultValueLiteral="0.0">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Minimum overhead for a context switch between schedulable resources."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Timetable_Driven_Packet_Based_Policy"
