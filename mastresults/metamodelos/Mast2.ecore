  <eClassifiers xsi:type="ecore:EClass" name="Scheduler" abstract="true" eSuperTypes="#//Model_Element">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="It models the operating system or network subsystem objects that implement the appropriate scheduling strategies to manage the amount of processing capacity that has been assigned to them. Its scheduling strategy is defined by means of the associated Scheduling_Policy object. Schedulers can have a hierarchical structure. A Primary Scheduler operates by offering the whole processing capacity of its associated base processor to its associated schedulable resources. A Secondary Scheduler is allowed to deliver to its schedulable resources just the processing capacity that it receives from its associated schedulable resource, scheduled in turn by some other scheduler."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Policy" ordered="false"
        lowerBound="1" eType="#//Scheduling_Policy" containment="true" eOpposite="#//Scheduling_Policy/Owner">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="It represents the scheduling policy, which is the basic strategy that is implemented by the scheduler to distribute the assigned processing capacity among the schedulable resources associated to the same scheduler."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Schedulable_Resource_List"
        lowerBound="1" upperBound="-1" eType="#//Schedulable_Resource" eOpposite="#//Schedulable_Resource/Scheduler"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Primary_Scheduler" eSuperTypes="#//Scheduler">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="It represents the base system scheduler for its associated processing resource."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Host" ordered="false" lowerBound="1"
        eType="#//Processing_Resource" eOpposite="#//Processing_Resource/Scheduler">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="It references to the processing resource that supplies the processing capacity to be scheduled."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Secondary_Scheduler" eSuperTypes="#//Scheduler">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="It is allowed to deliver to its schedulable resources just the processing capacity that it receives from its associated schedulable resource, scheduled in turn by some other scheduler."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Host" ordered="false" lowerBound="1"
        eType="#//Schedulable_Resource">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
