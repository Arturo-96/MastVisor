        <details key="documentation" value="Start time of the window inside the partition (relative to the start of the major frame (MAF) used as the origin of time for the partitions)."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Length" ordered="false"
        lowerBound="1" eType="#//Time">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Duration of the window."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Timetable_Driven_Policy" eSuperTypes="#//Timetable_Driven">
