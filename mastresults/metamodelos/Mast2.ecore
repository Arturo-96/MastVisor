        lowerBound="1" eType="#//Assertion" defaultValueLiteral="false">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="If this parameter is set to the value 'false', the deadline may be assigned by one of the deadline assignment tools. Otherwise, the deadline is fixed and cannot be changed by those tools."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="EDF_Params" eSuperTypes="#//EDF_Based_Params">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Concrete EDF scheduling parameters."/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Resource_Reservation_Params" abstract="true"
      eSuperTypes="#//Scheduling_Parameters">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="The scheduling parameters are only applicable to virtual schedulable resources (Virtual_Communication_Channel or Virtual_ Schedulable_Resource) as their Resource_Reservation_Params association."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Preassigned" ordered="false"
        lowerBound="1" eType="#//Assertion" defaultValueLiteral="false">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="If this parameter is set to the value 'false', the scheduling parameters may be assigned by one of the scheduling design tools. Otherwise, the parameters are fixed and cannot be changed by those tools."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Virtual_Resource_Params" abstract="true"
      eSuperTypes="#//Resource_Reservation_Params">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Resource reservation contract for a Resource Reservation policy.  It represents a service contract that contains the information related to the application minimum resource requirements."/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Virtual_Comm_Channel_Params" abstract="true"
      eSuperTypes="#//Resource_Reservation_Params">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
