        <details key="documentation" value="Minimun technological latency in reception in the AFDX hardware."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Avg_HW_Rx_Latency" ordered="false"
        lowerBound="1" eType="#//Time" defaultValueLiteral="1.7976931348623157E308">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Average technological latency in reception in the AFDX hardware."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Ethernet_Overhead" ordered="false"
        lowerBound="1" eType="#//Bit_Count" defaultValueLiteral="160">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="It is the number of overhead bytes to be added to each Ethernet frame. Its value is 160 bits (20 bytes)."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Protocol_Overhead" ordered="false"
        lowerBound="1" eType="#//Bit_Count" defaultValueLiteral="376">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="It is the number of overhead bytes corresponding to the protocol used for communications. Its value is 376 bits (47 bytes) for the UDP/IP used in AFDX."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Driver" abstract="true">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="It represents activities executed in a processor as a consequence of the transmission or reception of a message or a message packet through a network."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Owner" lowerBound="1" eType="#//Network"
        eOpposite="#//Network/Driver_List"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Packet_Driver" eSuperTypes="#//Driver">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="It represents a driver that is activated at each message packet transmission or reception."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Message_Segmentation" ordered="false"
        lowerBound="1" eType="#//Assertion" defaultValueLiteral="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="A 'false|false' value that determines whether or not the driver is capable of partitioning long messages into several packets and rebuilding the message at the other end. This attribute influences the overhead model of the driver."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="High_Utilization_Mode"
        ordered="false" lowerBound="1" eType="#//Assertion" defaultValueLiteral="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="This parameter specifies whether or not the network is used at a high transmission rate. Its value determines the overhead model that should be used for the driver. If the value is true, the system has a high network utilization and few messages are being partitioned (i.e., mostly short messages); in this case the driver overhead is modeled  as periodic send and receive operations with a period equal to the minimum packet transmission time (this is called the decoupled overhead model). If the value is false, the driver overhead is modeled as  message send and message receive operations that are attached to the transaction that causes the transmission of the message (this is called the coupled overhead model). Both models are pessimistic, but depending on the network utilization one of them produces more accurate results. The default value is true."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Packet_Send_Operation"
        ordered="false" eType="#//Code_Operation">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Reference to the operation that is executed by the packet server each time a packet is sent"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Packet_Receive_Operation"
        ordered="false" eType="#//Code_Operation">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Reference to the operation that is executed by the packet server each time a packet is received."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Packet_Server" ordered="false"
        eType="#//Thread">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The reference to the schedulable resource that is executing the part of the driver that is executed for each packet sent or received (which in turn has a reference to the scheduler (and indirectly to the processor), and to the scheduling parameters)."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Character_Packet_Driver" eSuperTypes="#//Packet_Driver">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="It is a specialization of a packet driver in which there is an additional overhead associated with sending each character, as happens in some serial lines."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Character_Transmission_Time"
        ordered="false" lowerBound="1" eType="#//Time" defaultValueLiteral="0.0">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Time of character transmission. It determines the period used for the overhead models of the character send and character receive operations."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Character_Send_Operation"
        ordered="false" eType="#//Code_Operation">
