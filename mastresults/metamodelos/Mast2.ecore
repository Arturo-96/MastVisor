    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Partition_Name" ordered="false"
        lowerBound="1" eType="#//Identifier" iD="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Optional identifier that is used as the partition name, for expressiveness purposes. Each partition name must correspond to a unique partition Id. "/>
      </eAnnotations>
