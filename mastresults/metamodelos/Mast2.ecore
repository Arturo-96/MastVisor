      <details key="documentation" value="Natural integer of model-defined range that must be within [0..2^15-1], defining the scheduling priority of interrupt service routines."/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EDataType" name="Natural" instanceClassName="int">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Integer number that is greater than or equal to zero."/>
