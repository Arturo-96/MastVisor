  <eClassifiers xsi:type="ecore:EClass" name="AFDX_Virtual_Link" eSuperTypes="#//Scheduling_Parameters">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="This scheduling parameters are only applicable to schedulers with AFDX Policy."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="LMax" ordered="false" lowerBound="1"
        eType="#//Bit_Count" defaultValueLiteral="0">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Maximum length of the packets sent through this virtual link."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="BAG" ordered="false" lowerBound="1"
