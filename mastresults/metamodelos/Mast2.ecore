      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Average  technological latency in transmission in the AFDX hardware."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Max_HW_Rx_Latency" ordered="false"
        lowerBound="1" eType="#//Time" defaultValueLiteral="1.7976931348623157E308">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Maximum  technological latency in reception in the AFDX hardware."/>
