    <eStructuralFeatures xsi:type="ecore:EReference" name="Owner_CO" eType="#//Composite_Observer"
        eOpposite="#//Composite_Observer/Observer_List"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Timing_Requirement" abstract="true"
      eSuperTypes="#//Observer">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="It represents requirements imposed on the instant of generation of the associated internal event."/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Queue_Size_Req" eSuperTypes="#//Observer">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="It represents a requirement imposed on the maximum number of event instances that may be queued. It is used for internal events that activate a Step and, therefore, it represents the maximum number of pending activations of that Step."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Max_Events" ordered="false"
        lowerBound="1" eType="#//Natural" defaultValueLiteral="0">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Maximum number of queued event instances that are admitted."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Deadline_Req" abstract="true" eSuperTypes="#//Timing_Requirement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="It represents a requirement imposed on the maximum time value allowed for the generation of the associated event. It is expressed as a relative time interval relative to the generation of another event."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Deadline" ordered="false"
        lowerBound="1" eType="#//Time" defaultValueLiteral="0.0">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The deadline value."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Hard_Local_Deadline" eSuperTypes="#//Deadline_Req">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="As local, it appears associated with the output event of a step; the deadline is relative to the arrival of the event that activated that step. As hard, it must be met in all cases, including the worst case."/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Hard_Global_Deadline" eSuperTypes="#//Deadline_Req">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="As global, the deadline is relative to the workload event that activates the current execution. As hard, it must be met in all cases, including the worst case."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Referenced_Event" ordered="false"
        lowerBound="1" eType="#//Workload_Event">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Workload event used as the time reference for the deadline."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Hard_Global_Relative_Deadline" eSuperTypes="#//Hard_Global_Deadline"/>
  <eClassifiers xsi:type="ecore:EClass" name="Soft_Local_Deadline" eSuperTypes="#//Deadline_Req">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="As local, it appears associated with the output event of a step; the deadline is relative to the arrival of the event that activated that step. As soft, it must be met on average."/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Soft_Global_Deadline" eSuperTypes="#//Deadline_Req">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="As global, the deadline is relative to a workload event that activates the current execution. As soft, it must be met on average."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Referenced_Event" ordered="false"
        lowerBound="1" eType="#//Workload_Event">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Workload event used as the time reference for the deadline."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Soft_Global_Relative_Deadline" eSuperTypes="#//Soft_Global_Deadline"/>
  <eClassifiers xsi:type="ecore:EClass" name="Max_Output_Jitter_Req" eSuperTypes="#//Timing_Requirement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="It represents a requirement for limiting the output jitter with which the associated periodic internal event is generated. Output jitter is calculated as the difference between the worst-case response time and the best-case response time for the associated event, relative to a Referenced Event that is an attribute of this requirement."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Max_Output_Jitter" ordered="false"
        lowerBound="1" eType="#//Time" defaultValueLiteral="0.0">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Mximum jitter required."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Referenced_Event" ordered="false"
        lowerBound="1" eType="#//Workload_Event">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Workload event used as the time reference for the deadline."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Max_Miss_Ratio_Req" abstract="true"
      eSuperTypes="#//Timing_Requirement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="It represents a kind of soft deadline in which the deadline cannot be missed more often than a specified ratio."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Deadline" ordered="false"
        lowerBound="1" eType="#//Time" defaultValueLiteral="0.0">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The deadline value."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Ratio" ordered="false"
        lowerBound="1" eType="#//Percentage" defaultValueLiteral="0.05">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Percentage representing the maximum ratio of missed deadlines"/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Global_Max_Miss_Ratio" eSuperTypes="#//Max_Miss_Ratio_Req">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Concrete Max_Miss_Ratio_Req, in which the deadline is relative to the Referenced Event."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Referenced_Event" ordered="false"
        lowerBound="1" eType="#//Workload_Event">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Workload event that is used as the reference for the deadline."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Local_Max_Miss_Ratio" eSuperTypes="#//Max_Miss_Ratio_Req">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Concrete Max_Miss_Ratio_Req, in which the deadline is relative to the activation of the step to which the timing requirement is attached."/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Composite_Observer" eSuperTypes="#//Observer">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="An internal event may have several requirements imposed at the same time, which are expressed via a composite observer. It is just a list of simple observers, i.e., non-composite observers."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Observer_List" ordered="false"
        lowerBound="1" upperBound="-1" eType="#//Observer" containment="true" eOpposite="#//Observer/Owner_CO">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="List of simple observers."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Event_Handler" abstract="true" eSuperTypes="#//Flow_Element">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Event handlers represent actions that are activated by the arrival of one or more events, and that in turn generate one or more events at their output. There are two fundamental classes of event handlers. The Steps represent the execution of an operation by a schedulable resource, in a processing resource, and with some given scheduling parameters. The other event handlers are just a mechanism for handling events, with no runtime effects."/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Step" eSuperTypes="#//Event_Handler">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="It represents the execution of an operation by a schedulable resource, in a processing resource, and with some given scheduling parameters"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Hold_Schedulable_Resource"
        ordered="false" lowerBound="1" eType="#//Assertion" defaultValueLiteral="false">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="IIf this attribute is set, the thread used by the step is not released at the end of the execution of the step operation. The thread is held until the next step in which the attribute is set to &quot;false&quot; or until the end-to-end flow is finished. This attribute only has effect for schedulable resources of the Thread class. The default value is false."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Input_Event" ordered="false"
        lowerBound="1" eType="#//Event">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="It references the event that activates the execution of the step"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Output_Event" ordered="false"
        lowerBound="1" eType="#//Internal_Event">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="It references the internal event that is generated when the step is finished."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Step_Operation" ordered="false"
        lowerBound="1" eType="#//Operation">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="It references the operation."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Step_Schedulable_Resource"
        ordered="false" lowerBound="1" eType="#//Schedulable_Resource">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Reference to the schedulable resource (which in turn contains references to the scheduling parameters and the processing resource). "/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Delay" eSuperTypes="#//Event_Handler">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="It is an event handler that generates its output event after a time interval has elapsed from the arrival of the input even. Note that the time intervals for different event instances are independently computed, even if they overlap. The delay may be handled by a Timer or by the environment (when the Timer reference is null)."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Delay_Max_Interval" ordered="false"
        lowerBound="1" eType="#//Time" defaultValueLiteral="0.0">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Longest time interval used to generate the output event."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Delay_Avg_Interval" ordered="false"
        lowerBound="1" eType="#//Time" defaultValueLiteral="0.0">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Average time interval used to generate the output event."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Delay_Best_Interval" ordered="false"
        lowerBound="1" eType="#//Time" defaultValueLiteral="0.0">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Shortest time interval used to generate the output event."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Input_Event" ordered="false"
        lowerBound="1" eType="#//Event">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="It references the event that activates the step. "/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Output_Event" ordered="false"
        lowerBound="1" eType="#//Internal_Event">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="It references the event that is generated when the step is finished."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Timer" ordered="false"
        eType="#//Timer">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Reference to the timer, used when the delay has an overhead that must be modeled. The default is NULL, indicating that the overhead associated to the delay operation is negligible."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Offset" eSuperTypes="#//Delay">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="It is similar to the Delay event handler, except that the time interval is counted relative to the arrival of some (previous) workload event. If the time interval has already passed when the input event arrives, the output event is generated immediately."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Referenced_Event" ordered="false"
        lowerBound="1" eType="#//Workload_Event">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Reference to the workload event that is used as temporal reference. "/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Rate_Divisor" eSuperTypes="#//Event_Handler">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="It is an event handler that generates one output event when a number of input events equal to the Rate Factor have arrived."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Rate_Factor" ordered="false"
        lowerBound="1" eType="#//Positive" defaultValueLiteral="1">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Number of events that must arrive to generate an output event."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Input_Event" ordered="false"
        lowerBound="1" eType="#//Event">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="It references the input event for the event handler. "/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Output_Event" ordered="false"
        lowerBound="1" eType="#//Internal_Event">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="references the event that is generated as output."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Fork" eSuperTypes="#//Event_Handler">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="It is an event handler that generates one event in each of its outputs each time an input event arrives. If the step previous to the fork handler has a 'true' value assigned to its Hold_Schedulable_Resource attribute, only one of the output paths following the fork handler can have Steps assigned to the same Schedulable_Resource as that step. The schedulable resource is unlocked when one of those Steps has a 'false' value assigned to its Hold_Schedulable_Resource attribute, or when the corresponding output path terminates."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Input_Event" ordered="false"
        lowerBound="1" eType="#//Event">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="It references the input event the event handler. "/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Output_Event_List" ordered="false"
        lowerBound="1" upperBound="-1" eType="#//Internal_Event">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="List of references to the output events."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Join" eSuperTypes="#//Event_Handler">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Itis an event handler that generates its output event when all of its input events have arrived. For worst-case analysis to be possible it is necessary that all the input events are periodic with the same periods. This usually represents no problem if the event handler is used to perform a 'join' operation after a 'fork' operation carried out with the Fork event handler. When the EDF scheduling policy is being used, the generation time of the event used to obtain the absolute deadlines is set to the most recent of the generation times of the input event instances."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Input_Event_List" ordered="false"
        lowerBound="1" upperBound="-1" eType="#//Event">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="List of references to the input events. "/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Output_Event" ordered="false"
        lowerBound="1" eType="#//Internal_Event">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="It references the event that is generated as output."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Branch" eSuperTypes="#//Event_Handler">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="It is an event handler that generates one event in only one of its outputs each time an input event arrives. The output path is chosen at the time of the event generation."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Output_Weight_List" ordered="false"
        lowerBound="1" upperBound="-1" eType="#//Float">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="List of weights for each output.For a RANDOM policy, if the weights are w1, w2, ..., wi, the probability of choosing a particular output j is wj/wT, where wT is the total weight, wT=w1+w2+...+wi If policy is SCAN, the weight must be integer; if the weigths for this case are w1, w2, ..., wi, the first w1 events will be directed through the first output, the next w2 events through the second output, and so on. The default value for the weight of each output is 1.."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Delivery_Policy" ordered="false"
        lowerBound="1" eType="#//Delivery_Policy_Type" defaultValueLiteral="RANDOM">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The policy used to determine the output path."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Input_Event" ordered="false"
        lowerBound="1" eType="#//Event">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Reference to the input event. "/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Output_Event_List" ordered="false"
        lowerBound="1" upperBound="-1" eType="#//Internal_Event">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="List of references to the output events."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Merge" eSuperTypes="#//Event_Handler">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="It is an event handler that generates its output event every time one of its input events arrives. When several pending event instances are queued in the Merge output, the policy assigned to the Event_Queueing_Policy attribute of the MAST_Model is used to determine which request must be processed first by the following step."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Input_Event_List" ordered="false"
        lowerBound="1" upperBound="-1" eType="#//Event">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="List of references to input events. "/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Output_Event" ordered="false"
        lowerBound="1" eType="#//Internal_Event">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="It references the event that is generated as output."/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Queried_Branch" eSuperTypes="#//Event_Handler">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="It is an event handler that generates one event in only one of its outputs each time an input event arrives. The output path is chosen at the time of the event consumption by one of the Steps connected to an output event."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Request_Policy" ordered="false"
        lowerBound="1" eType="#//Request_Policy_Type" defaultValueLiteral="SCAN">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The policy used to determine the output path when there are several pending requests from the connected steps. "/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Input_Event" ordered="false"
        lowerBound="1" eType="#//Event"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Output_Event_List" ordered="false"
        lowerBound="1" upperBound="-1" eType="#//Internal_Event"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Message_Event_Handler" abstract="true"
      eSuperTypes="#//Event_Handler">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Event_Handler that handles the messages in switches and routers."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Switch" ordered="false"
        lowerBound="1" eType="#//Network_Switch">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Reference to the switch in which the transfer is executed. "/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Message_Fork" eSuperTypes="#//Message_Event_Handler">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="An input message is transmitted through several output networks; it is a multicast operation."/>
