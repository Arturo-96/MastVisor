    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Worst_Context_Switch" ordered="false"
        lowerBound="1" eType="#//Normalized_Execution_Time" defaultValueLiteral="1.7976931348623157E308">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Maximum overhead for a context switch between schedulable resources."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Avg_Context_Switch" ordered="false"
        lowerBound="1" eType="#//Normalized_Execution_Time" defaultValueLiteral="0.0">
