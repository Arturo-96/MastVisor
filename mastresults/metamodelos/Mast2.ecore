  <eClassifiers xsi:type="ecore:EClass" name="EDF_Policy" eSuperTypes="#//Scheduling_Policy">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="It represents an Earliest Deadline First policy. In the EDF policy each schedulable resource is assigned a relative deadline, and is scheduled with an absolute deadline that is equal to the relative deadline plus the generation time of the event instance being executed. The generation time can be accounted for in two different ways, leading to the so called Global EDF or Local EDF variants. In Global EDF the event generation time corresponds to the generation time of the workload event that is being processed. This time is meaningful only for steps in the same processing resource where the workload event was generated, or in processing resources that have their system timers synchronized with it. However, it is not meaningful in processing resources that are not synchronized with the processing resource where the workload event was generated. For these non-synchronized resources local EDF is assumed, implying that the generation time of the event is the time at which the internal event being processed arrived in the processing resource. The EDF Policy can only be assigned to a scheduler that has a Regular_Processor as its host."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Worst_Context_Switch" ordered="false"
        lowerBound="1" eType="#//Normalized_Execution_Time" defaultValueLiteral="0.0">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Maximum overhead for context switch between schedulable resources."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Avg_Context_Switch" ordered="false"
        lowerBound="1" eType="#//Normalized_Execution_Time" defaultValueLiteral="0.0">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Average overhead for context switch between schedulable resources."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Best_Context_Switch" ordered="false"
        lowerBound="1" eType="#//Normalized_Execution_Time" defaultValueLiteral="0.0">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Minimum overhead for context switch between schedulable resources."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Is_Global" lowerBound="1"
        eType="#//Assertion" defaultValueLiteral="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="FP_Packet_Based_Policy" eSuperTypes="#//Scheduling_Policy">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="It represents a fixed priority policy used in a packet oriented communication network. Network packets are assumed to be non preemptible. It can onlybe assigned to a scheduler that has a Packet_Based_Network as its host."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Max_Priority" ordered="false"
        lowerBound="1" eType="#//Priority" defaultValueLiteral="32767">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="It defines the maximum priority valid for messages to be sent using this policy."/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Min_Priority" ordered="false"
        lowerBound="1" eType="#//Priority" defaultValueLiteral="1">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="It defines the minimum priority valid for messages to be sent using this policy."/>
